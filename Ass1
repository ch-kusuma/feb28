def OddOccurrences(arr,n):
    for i in range(0,n):
        count = 0
        for j in range(0,n):
            if arr[i] == arr[j]:
                count+=1
        if (count % 2 != 0):
            return arr[i]
    return -1
arr = [1,2,3,2,1,3,2,1,2,2 ]
n = len(arr)
print(OddOccurrences(arr, n))



